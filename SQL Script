                 WHEN EXTRACT(MONTH from started_at) = 12 THEN 'B'
                 WHEN EXTRACT(MONTH from started_at) = 1 THEN 'C'
                 WHEN EXTRACT(MONTH from started_at) = 2 THEN 'D'
                 WHEN EXTRACT(MONTH from started_at) = 3 THEN 'E'
                 WHEN EXTRACT(MONTH from started_at) = 4 THEN 'F'
