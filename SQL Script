  SELECT *, CASE WHEN EXTRACT(DAYOFWEEK from started_at) BETWEEN 2 AND 6 THEN 'weekday'
              ELSE 'weekend' END AS weekpart, TIMESTAMP_DIFF(ended_at, started_at, SECOND) AS ridelength
  FROM `decoded-cove-381306.Months.oct2022`)

----------"weekday" column lists whether a ride took place on a weekday or a weekend (Saturday or Sunday)----------

  SELECT member_casual, weekpart, COUNT(*)/(SUM(COUNT(*)) OVER(PARTITION BY member_casual)) AS pct_of_membertype
  FROM (SELECT *
        FROM allmonths
        WHERE allmonths.ridelength > 0 AND allmonths.ridelength < 86400)
  GROUP BY member_casual, weekpart
  ORDER BY member_casual, weekpart;
  
  
  
  
