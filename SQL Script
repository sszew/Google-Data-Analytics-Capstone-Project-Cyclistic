                 WHEN EXTRACT(MONTH from started_at) = 4 THEN 'F'
                 WHEN EXTRACT(MONTH from started_at) = 5 THEN 'G'
                 WHEN EXTRACT(MONTH from started_at) = 6 THEN 'H'
                 WHEN EXTRACT(MONTH from started_at) = 7 THEN 'I'
                 WHEN EXTRACT(MONTH from started_at) = 8 THEN 'J'
                 WHEN EXTRACT(MONTH from started_at) = 9 THEN 'K'
                 ELSE 'L' END AS month, 
          TIMESTAMP_DIFF(ended_at, started_at, SECOND) AS ridelength
  FROM `decoded-cove-381306.Months.oct2022`)

----------In order to maintain the correct chronology of events, I assigned each month to a letter: ----------
----------A = Nov. '21, B = Dec. '21, C = Jan. '22, D = Feb. '22, E = Mar. '22, F = Apr. '22 ----------
----------G = May '22, H = Jun. '22, I = Jul. '22, J = Aug. '22, K = Sep. '22, L = Oct. '22 ----------

SELECT member_casual, STDDEV(med_ridelength) AS sd_med_ridelength
FROM (
  SELECT member_casual, month, median_ridelength/60 AS med_ridelength
  FROM (SELECT *, PERCENTILE_CONT(ridelength, 0.5) OVER(PARTITION BY member_casual, month) AS median_ridelength
        FROM allmonths
        WHERE allmonths.ridelength > 0 AND allmonths.ridelength < 86400)
  GROUP BY member_casual, month, med_ridelength
  ORDER BY member_casual, month)
GROUP BY member_casual;
  
                                                  

