  UNION ALL
  SELECT *, CASE WHEN EXTRACT(DAYOFWEEK from started_at) BETWEEN 2 AND 6 THEN 'weekday'
              ELSE 'weekend' END AS weekpart, TIMESTAMP_DIFF(ended_at, started_at, SECOND) AS ridelength
  FROM `decoded-cove-381306.Months.mar2022`
  UNION ALL
  SELECT *, CASE WHEN EXTRACT(DAYOFWEEK from started_at) BETWEEN 2 AND 6 THEN 'weekday'
              ELSE 'weekend' END AS weekpart, TIMESTAMP_DIFF(ended_at, started_at, SECOND) AS ridelength
  FROM `decoded-cove-381306.Months.apr2022`
  UNION ALL
  SELECT *, CASE WHEN EXTRACT(DAYOFWEEK from started_at) BETWEEN 2 AND 6 THEN 'weekday'
              ELSE 'weekend' END AS weekpart, TIMESTAMP_DIFF(ended_at, started_at, SECOND) AS ridelength
  FROM `decoded-cove-381306.Months.may2022`
  UNION ALL
  SELECT *, CASE WHEN EXTRACT(DAYOFWEEK from started_at) BETWEEN 2 AND 6 THEN 'weekday'
              ELSE 'weekend' END AS weekpart, TIMESTAMP_DIFF(ended_at, started_at, SECOND) AS ridelength
